{
  "name": "rstgroup/object-builder",
  "description": "Dynamic build of object",
  "license": "MIT",
  "keywords": [],
  "require": {
    "php": "^7.2",
    "phpstan/phpdoc-parser": "^0.3.0",
    "roave/better-reflection": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "RstGroup\\ObjectBuilder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RstGroup\\ObjectBuilder\\Test\\": "test/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "check": [
      "@test"
    ],
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  }
}
